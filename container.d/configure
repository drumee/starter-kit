#!/bin/bash
# This script in intended to run within container
set -e
script_dir=$(dirname $(readlink -f $0))

if [ -f /etc/drumee/drumee.sh ]; then
  echo Drumee is already configured
  exit 0
fi


cd $script_dir
node setup-infra/index.js --localhost=1 --db-dir=/home/somanos/github/starter-kit/storage/db
